int romanToInt(string s) {
    // Write your code here
    int n = s.length();
    if(n == 0) return 0;
    
    unordered_map<char, int> mpp = {
        {'I',1}, {'V',5}, {'X',10}, {'L',50}, {'C',100}, {'D',500}, {'M',1000}
    };

    int number = mpp[s[n-1]];
    
    for(int i = n-2 ; i >= 0 ; i--) {
        if(mpp[s[i]] >= mpp[s[i+1]]) number += mpp[s[i]];
        else number -= mpp[s[i]];
    }
    return number;
}
